[
  {
    "path": "/api/auth/loginold",
    "method": "POST",
    "roles": ["all"],
    "params": [
      {
        "name": "userEmail",
        "type": "string",
        "pattern": "[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,4}$",
        "required": true
      },
      {
        "name": "password",
        "type": "string",
        "pattern": "^(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]{6,15}$",
        "required": true
      }
    ]
  },
  {
    "path": "/api/auth/login",
    "method": "POST",
    "roles": [
      "admin"
    ],
    "params": [],
    "bodyValidation": {
      "feesName": "^[a-zA-Z0-9 ]{3,30}$"
    }
  }
]