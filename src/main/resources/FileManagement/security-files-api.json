{
  "urls": [
    {
      "path": "/api/files/uploadFile",
      "method": "POST",
      "roles": ["FileMgmt_Read"],
      "template": "uploadFileTemplate"
    }
  ],
  "templates": [
    {
      "name": "uploadFileTemplate",
      "type": "JSONObject",
      "params": [
        {
          "name": "file",
          "type": "file",
          "pattern": "pdf,doc,xls,xlsx,img,jpeg,gif,csv,zip,tar,7z,svg,png",
          "required": true,
          "size": 1000000

        }
      ]
    }
  ]
}